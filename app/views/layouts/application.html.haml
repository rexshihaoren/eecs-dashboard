!!! 5
%html
  %head
    %title EECS Dashboard
    = stylesheet_link_tag 'application'
    = javascript_include_tag 'application'
    = javascript_include_tag 'jquery-1.10.2'
    = javascript_include_tag 'highcharts'
    = javascript_include_tag 'jquery-ui-1.10.3/ui/jquery-ui.js
    = csrf_meta_tags

  %body
    %div#header
    %img{:src => "/images/iris_logo.png", :id => "iris_logo"}
    %img{:src => "/images/eecs_logo.png", :id => "eecs_logo"}
    %img{:src => "/images/berkeley_logo.png", :id => "berkeley_logo"}
    %h1.title EECS Dashboard
   
    #main
      - if flash[:notice]
        #notice.message= flash[:notice]
      - elsif flash[:warning]
        #warning.message= flash[:warning]

      = yield
